---
<%
CONFIG_PATH = "/var/vcap/jobs/service-fabrik-serviceflow-manager/config"
%>
processes:
- name: service-fabrik-serviceflow-manager
  executable: /var/vcap/packages/node/bin/node
  args:
  - /var/vcap/packages/service-fabrik-broker/broker/applications/operators/src/serviceflow-operator/StartSerialServiceFlow.js
  env:
    CONF_DIR: <%= CONFIG_PATH %>
    NODE_ENV: production
    SETTINGS_PATH: <%= CONFIG_PATH %>/settings.yml
