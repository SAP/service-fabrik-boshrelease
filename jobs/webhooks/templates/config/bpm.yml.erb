---
<%
KUBECONFIG_PATH = "/var/vcap/jobs/service-fabrik-apiserver/config"
%>
processes:
- name: webhooks
  executable: /var/vcap/packages/webhooks/bin/manager-webhooks
  args:
  - -stderrthreshold=INFO
  env:
    KUBECONFIG: <%= KUBECONFIG_PATH %>/kubeconfig
  limits:
    open_files: 100000
