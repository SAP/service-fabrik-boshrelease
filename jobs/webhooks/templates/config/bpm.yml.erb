---
<%
CONFIG_PATH = "/var/vcap/jobs/webhooks/config"
%>
processes:
- name: webhooks
  executable: /var/vcap/packages/webhooks/bin/manager-webhooks
  args:
  - -stderrthreshold=<%= p('log_level') %>
  env:
    KUBECONFIG: <%= CONFIG_PATH %>/kubeconfig.yaml
  limits:
    open_files: 100000
